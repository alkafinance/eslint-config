{
  "name": "@alkafinance/eslint-config-react",
  "version": "0.0.0-development",
  "description": "Alka ESLint config for our React/React Native apps",
  "keywords": [
    "eslint",
    "eslint config",
    "config",
    "alka",
    "styleguide"
  ],
  "homepage": "https://github.com/alkafinance/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/alkafinance/eslint-config/issues"
  },
  "license": "MIT",
  "author": "Ayan Yenbekbay <ayan@alka.app>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alkafinance/eslint-config.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "find-new-rules:web": "eslint-find-rules --unused test/fixtures/web.js",
    "find-new-rules:native": "eslint-find-rules --unused test/fixtures/native.js",
    "find-new-rules": "run-p --silent find-new-rules:*",
    "semantic-release": "semantic-release"
  },
  "release": {
    "extends": "semantic-release-monorepo"
  },
  "peerDependencies": {
    "@alkafinance/eslint-config": "*"
  },
  "dependencies": {
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-react-native": "^3.8.1"
  },
  "devDependencies": {}
}
