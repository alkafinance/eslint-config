{
  "name": "@alkafinance/eslint-config-react",
  "version": "0.0.0-development",
  "description": "Alka ESLint config for our React/React Native apps",
  "keywords": [
    "eslint",
    "eslint config",
    "config",
    "alka",
    "styleguide"
  ],
  "repository": "alkafinance/eslint-config",
  "homepage": "https://github.com/alkafinance/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/alkafinance/eslint-config/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan@alka.app"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "scripts": {
    "find-new-rules:index": "eslint-find-rules --unused test/fixtures/index.js",
    "find-new-rules:native": "eslint-find-rules --unused test/fixtures/native.js",
    "find-new-rules": "run-p --silent find-new-rules:*",
    "semantic-release": "semantic-release"
  },
  "release": {
    "extends": "semantic-release-monorepo"
  },
  "peerDependencies": {
    "@alkafinance/eslint-config": "*"
  },
  "dependencies": {
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-native": "^3.7.0"
  },
  "devDependencies": {}
}
